{
  "expo": {
    "name": "Advancedweighttracker",
    "slug": "advanced-weight-tracker",
    "scheme": "advancedweighttracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Advancedweighttracker.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.awt.advancedweighttracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.awt.advancedweighttracker",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-8833281523608204~4805371943"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "react-native-google-mobile-ads"
    ]
  }
}